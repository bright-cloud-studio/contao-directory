{
    "name":"bright-cloud-studio/contao-directory",
    "description":"Users can make submissions to a directory with a module to display and filter the results.",
    "homepage": "http://brightcloudstudio.com/",
    "keywords":["directory", "bright", "cloud"],
    "type":"contao-module",
    "license":"LGPL-3.0-or-later",
    "authors":[
        {
            "name":"Bright Cloud Studio",
            "homepage":"https://brightcloudstudio.com"
        }
    ],
    "require":{
        "php": "^8.0",
        "contao/core-bundle": "^4.13"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Bcs\\": [
                "system/modules/contao_directory/library/Bcs"
            ]
        }
    },
    "extra": {
        "contao": {
            "sources": {
                "system/modules/contao_directory": "system/modules/contao_directory"
            }
        }
    },
    "minimum-stability": "dev"
}
